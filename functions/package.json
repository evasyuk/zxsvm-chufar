{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "debug": "MODE=DEV scripts/generate.app.config.sh && node debug.js",
    "serve": "MODE=DEV scripts/generate.app.config.sh && firebase emulators:start --only functions",
    "deploy": "MODE=PROD scripts/generate.app.config.sh && firebase deploy --only functions",
    "shell": "firebase functions:shell",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "main": "index.js",
  "dependencies": {
    "@babel/cli": "7.8.4",
    "@babel/core": "7.8.7",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.10.4",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "7.8.7",
    "@babel/register": "^7.12.1",
    "axios": "^0.21.0",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "firebase-admin": "^8.10.0",
    "firebase-functions": "^3.6.1",
    "google-spreadsheet": "^3.0.13",
    "telegraf": "^3.38.0",
    "telegraf-inline-menu": "^5.3.0"
  },
  "devDependencies": {
    "@babel/node": "^7.8.7",
    "firebase-functions-test": "^0.2.0"
  },
  "private": true
}
